{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es2022",
        "allowJs": false,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "types": ["node"],
        "moduleDetection": "force",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}
